{
    "name": "wotz/filament-custom-tiptap-extensions",
    "description": "Some custom extensions for Filament Tiptap",
    "keywords": [
        "who owns the zebra",
        "laravel",
        "filament-custom-tiptap-extensions"
    ],
    "homepage": "https://github.com/wotzebra/filament-custom-tiptap-extensions",
    "license": "MIT",
    "support": {
        "email": "info@whoownsthezebra.be"
    },
    "require": {
        "php": "^8.2",
        "codeat3/blade-carbon-icons": "^2.38",
        "filament/filament": "^4.0|^5.0",
        "illuminate/contracts": "^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.12",
        "wotz/filament-link-picker": "^2.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0|^8.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Wotz\\FilamentCustomTiptapExtensions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wotz\\FilamentCustomTiptapExtensions\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wotz\\FilamentCustomTiptapExtensions\\Providers\\FilamentCustomTiptapExtensionsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
